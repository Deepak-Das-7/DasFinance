{
  "expo": {
    "name": "DasFinance",
    "slug": "dasFinance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/pngs/panther.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/pngs/DasPay.png",
        "backgroundColor": "#fff"
      },
      "package": "com.deepak7.dasFinance"
    },
    "bundler": "metro",
    "output": "static",
    "favicon": "./src/pngs/DasPay.png",
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/pngs/DasPay.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#fff"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
